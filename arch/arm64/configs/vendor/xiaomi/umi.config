# Device Specific Board Configuration
CONFIG_BOARD_UMI=y

# Touchscreen
CONFIG_TOUCHSCREEN_ST_FTS_V521=y
CONFIG_TOUCHSCREEN_FOD=y
# CONFIG_TOUCHSCREEN_ST_DEBUG_FS is not set
CONFIG_TOUCHSCREEN_GOODIX_GTX8=y
CONFIG_SECURE_TOUCH=y

# Fingerprint
CONFIG_FINGERPRINT_FPC_FOD=y
CONFIG_FINGERPRINT_GOODIX_FOD=y

# Enable QCOM RPMH Regulator
CONFIG_REGULATOR_QCOM_RPMH=y

# Enable PM8008 Regulator
CONFIG_REGULATOR_PM8008=y

# Hall sensor
CONFIG_HALL_AKM09970=y

# Power supply drivers
CONFIG_IDT_P9415=y
CONFIG_RX1619_TRIM=y
CONFIG_RX1619=y
CONFIG_LN8282=y
CONFIG_BQ2597X_CHARGE_PUMP=y
CONFIG_BQ_PUMP_WIRELESS_CHARGE=y
CONFIG_BATT_VERIFY_BY_DS28E16=y
CONFIG_ONEWIRE_GPIO=y
CONFIG_HL6111R=y

# Retry i2c operations on failure
CONFIG_I2C_RETRY=y

# Enable Awinic AW2xxx leds
CONFIG_LEDS_AW2015=y

# Camera
CONFIG_TECHPACK_CAMERA_XIAOMI=y

# SLMK
CONFIG_ANDROID_SIMPLE_LMK_MINFREE=64
